        </defaultConnector>
        <defaultConnectorLabel>Resp Ext ID does not exist</defaultConnectorLabel>
        <rules>
            <name>Resp_ExtID_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varResponseExtensionID</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varResponseExtensionID</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>RespExtensionIDAssignment</targetReference>
            </connector>
            <label>Resp ExtID Exists</label>
        </rules>
    </decisions>
