    <decisions>
        <name>If_Delivery_Mode_Exists</name>
        <label>If Delivery Mode Exists</label>
        <locationX>431</locationX>
        <locationY>804</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Delivery_Mode_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varDeliveryMode</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varDeliveryMode</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_WI_for_Delivery_Mode</targetReference>
            </connector>
            <label>Delivery Mode Exists</label>
        </rules>
    </decisions>
