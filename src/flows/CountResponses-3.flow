    <decisions>
        <description>This Decison check if response being key</description>
        <name>IsKeyResponse</name>
        <label>IsKeyResponse</label>
        <locationX>671</locationX>
        <locationY>215</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Key_Response</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>soResponse.Key__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>assCalcTotalKeyCount</targetReference>
            </connector>
            <label>Key Response</label>
        </rules>
        <rules>
            <name>Not_Key_Response</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>soResponse.Key__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>loopResponses</targetReference>
            </connector>
            <label>Not Key Response</label>
        </rules>
    </decisions>
    <decisions>
        <name>IsNetKeyResponse</name>
        <label>IsNetKeyResponse</label>
        <locationX>665</locationX>
        <locationY>335</locationY>
        <defaultConnector>
            <targetReference>loopResponses</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>NetKeyResponse</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>soResponse.Response_Status_Reason__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Out of Scope</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>assCalcTotalNetKeyCount</targetReference>
            </connector>
            <label>NetKeyResponse</label>
        </rules>
    </decisions>
    <decisions>
        <name>NetSampleResponse</name>
        <label>NetSampleResponse</label>
        <locationX>656</locationX>
        <locationY>58</locationY>
        <defaultConnector>
            <targetReference>IsKeyResponse</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>IsNetSampleResponse</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>soResponse.Response_Status_Reason__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Out of Scope</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>assCalcTotalNetSampleCount</targetReference>
            </connector>
            <label>IsNetSampleResponse</label>
        </rules>
    </decisions>
    <description>This flow combines CountKeyResponses, Count Net keys and Count Net Sample Size flows into 1 because all 3 were looping through responses individually could hit govenor limit of salesforce.</description>
    <interviewLabel>CountResponses {!$Flow.CurrentDateTime}</interviewLabel>
    <label>CountResponsesForCollectionInstance</label>
